package practice.algo.datastructure;

public class StringDuplicateCharacterReplacement {

	public static void main(String[] args) {
		System.out.println(charReplacement("ashish"));
	}

	public static String charReplacement(String strng) {

		char [] str = strng.toCharArray();

		if (str == null) 
			return;
		int len = str.length;
		if (len < 2) return;

		int tail = 1;
		for (int i = 1; i < len; ++i) {
			int j;
			for (j = 0; j < tail; ++j) {
				if (str[i] == str[j]) break;
			}
			if (j == tail) {
				str[tail] = str[i];
				++tail;
			}
		}
		str[tail] = 0;
	}


}
