package practice.algo.datastructure;

public class MatrixRotation {

	public static void main(String[] args) {
		int[][] a = {{1,2,3},{4,5,6},{7,8,9}};
		int n = 3 ;	
		rotateArray(a,n);
	}	
	
	
	public static void rotateArray(int [][] arr , int l) {
		for(int i = 0 ; i < l/2 ; i++) {
			int x = i ;
			int y = l-1-i ;
			for(int j = x ; j < y ; j ++) {
				
			}
			
			
		}
		
	}
}


